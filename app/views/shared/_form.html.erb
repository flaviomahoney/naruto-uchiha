<div class="container">
    <%= simple_form_for(@product) do |f| %>
            <%= f.input :name %>
            <%= f.input :photo, as: :file %>
            <%= f.input :description%>
            <%= f.input :size %>
            <%= f.input :price %>
            <%= f.button :submit , class: "btn btn-primary"%>
    <% end %>
</div>